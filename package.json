{
  "name": "gingertronmk1.github.io",
  "version": "1.0.0",
  "description": "A Jekyll website containing my CVs and other projects",
  "main": "index.js",
  "scripts": {
    "lint": "eslint _assets/js/**/* _assets/js/*.js",
    "fix": "eslint --fix _assets/js/**/* _assets/js/*.js",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GingertronMk1/gingertronmk1.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GingertronMk1/gingertronmk1.github.io/issues"
  },
  "homepage": "https://github.com/GingertronMk1/gingertronmk1.github.io#readme",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.22.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^7.8.0",
    "laravel-mix": "^5.0.9",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "browserslist": [
    "cover 100%"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "vue": "^2.6.12"
  }
}
